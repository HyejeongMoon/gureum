platform :osx, "10.10"
workspace 'Gureum'

target 'OSX' do
    project 'Gureum.xcodeproj'
    pod 'cdebug'
    pod 'FoundationExtension', '>= 1.3.0'
    pod 'Fabric'
    pod 'Crashlytics'
end
target 'OSXTests' do
    project 'Gureum.xcodeproj'
    pod 'cdebug'
    pod 'FoundationExtension', '>= 1.3.0'
end

target :'App' do
    platform :ios, "9.0"
    project 'iOS.xcodeproj'
    pod 'FoundationExtension', '>= 1.3.0'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'GoogleMobileAds'
    pod 'GlobalActivityIndicatorView', '>= 0.1.1'
end
target :'iOS' do
    platform :ios, "9.0"
    project 'iOS.xcodeproj'
    pod 'FoundationExtension', '>= 1.3.0'
    pod 'Fabric'
    pod 'Crashlytics'
end
